# This script is displaying "Holberton School" 10 times
#!/usr/bin/env bash
x=1
while [ $x -lt 101 ]
do
  c=$x%5
  t=$x%3
if [[ $c -eq "0" ]] && [[ $t -eq "0" ]]
then
echo "FizzBuzz"
elif [[ $t -eq "0" ]]
  then
  echo "Fizz"
elif [[ $c -eq "0" ]]
then
  echo "Buzz"
else
  echo "$x"
fi
x=$((x+1))
done
